.wrapper {
  height: 100vh; /* 设置容器高度为整个视窗高度 */
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.container {
  width: 359px;
  height: 515px;
  background-image: url("data:image/png;base64,");
}

.result-pic {
  width: 204px;
  height: 258px;
  background-image: url("https://static.fuxi.netease.com/yaotai/portal/index/lfb2krct_636796.png");
  position: relative;
}

.result-pic-content {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
.divination {
}
.title {
  margin-top: 20px;
  font-family: Kaiti SC;
  font-size: 28px;
  text-align: center;

  /* 主色/棕 */
  color: #7c4a28;
}

.head-line {
  width: 280px;
  height: 36px;
  text-align: center;
  line-height: 36px;
  background-image: url("data:image/png;base64,");
}

.result-detail {
  width: 265px;
  text-align: center;
}

.toolbar {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 100rpx;
  width: 100%;
}

.btn {
  background-image: url("data:image/png;base64,");
  height: 44px;
  width: 321px;
  line-height: 44px;
  border-radius: 4rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
}

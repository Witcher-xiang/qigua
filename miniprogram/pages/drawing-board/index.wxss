.canvas-wrapper {
  height: 100vh; /* 设置容器高度为整个视窗高度 */
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.canvas-container {
  width: 359px;
  height: 515px;
  background-image: url('data:image/png;base64,');
}

.canvas-box {
position: absolute;
top: 50px;
left: 35px;

width: 320px;
height: 470px;

  background-color: rgb(151, 106, 106);
}

.console{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 85%;
}
.console-left{

  display: flex;
  flex-direction: row;
  justify-content: space-around;
  width: 100vw;
}
.console-right{
margin-left: 10vw;
  width: 30%;
}
.console-icon{
  width: 40px;
  height: 38px;
}

.toolbar {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 100rpx;


  width: 100%;
}

.btn {

  background-image: url('data:image/png;base64,');
  height: 44px;
  width: 321px;
  line-height: 44px;
  border-radius: 4rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;

}
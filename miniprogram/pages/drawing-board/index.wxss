.canvas-wrapper {
  height: 100vh; /* 设置容器高度为整个视窗高度 */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}

.canvas-container {
  position: relative;
  width: 356px;
  height: 515px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  background-repeat: no-repeat;
  background-image: url('https://636c-clouddev-1g0cvf6m39a42001-1316562773.tcb.qcloud.la/draw-borad/img_paintingboard%403x.png?sign=2a460a86bd7dd402d8f2471180a834fd&t=1679822146');
}

.canvas-box {
  z-index: 1;
  font-family: Kaiti SC;
  position: relative;

}

.tips{
  margin: 10px;
  position: absolute;
  width: 300px;
  color: #CD9772;
  top: 20rpx;
}

.console{
  /* filter: grayscale(100%); */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 85%;
}
.console-left{

  display: flex;
  flex-direction: row;
  justify-content: space-around;
  width: 100vw;
}
.console-right{
margin-left: 10vw;
  width: 30%;
}
.console-icon{
  width: 40px;
  height: 38px;
}

.toolbar {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 100rpx;

  width: 100%;
}

.btn {
  background-image: url('data:image/png;base64,');
  height: 44px;
  width: 321px;
  line-height: 44px;
  border-radius: 4rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
}

.btn-disable{
  background-image: url('data:image/png;base64,');
  height: 44px;
  width: 321px;
  line-height: 44px;
  border-radius: 4rpx;
  color: #999999;
  font-size: 32rpx;
  text-align: center;
}





.receiveFile {
  margin-top: 20rpx;
  border-radius: 20px;
  background: #fff;
  color: #175acb;
  border: 2rpx solid #175acb;
}
.dowicon{
  width: 68px;
  height: 68px;
  margin-top: 50px;
  margin:20px 117px 0px 122px;
  text-align: center;
}
.inputcode{
  padding-left: 20rpx;
  height: 40px;
  width: 78%;
  color: #000;
  font-weight: normal;
  background-color: #F6F6FF;
  border: 1px solid rgb(231, 230, 230) ;
  margin: 17px ;
}

.isModalShow{
  display: block;
}
.isModalHide{
  display: none;
}
.modal{
  height: 100%;
  width: 100%;
  position: fixed;
  /* border: 1px solid springgreen; */
  background-color:rgba(0, 0, 0, .5); 
  top: 0;
  left: 0;
  z-index: 999;
}
.modal-content{
  top: 240rpx;
  padding: 56rpx 0px;
  width: 80%;
  height: 200rpx;
  background: #fff;
  margin: 20% auto;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  position: relative;
} 

.modal-content-title{
  font-family: Kaiti SC;
  font-size: 16px;
  color: #7C4A28;
  text-align: center;
}

.modal-content-button{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.yes{
  /* 直接获取本地图片无效 */
  background-image: url('../../img/modal/yes.png');
  height: 46px;
  width: 108px;
  line-height: 44px;
  border-radius: 4rpx;
  color: white;
  font-size: 32rpx;
  text-align: center;
  margin: 12px;
  background-repeat: no-repeat;
}

.cancel{
  /* 直接获取本地图片无效 */
  background-image: url('../../img/modal/cancel.png');
  height: 48px;
  line-height: 44px;
  border-radius: 4rpx;
  color: #7C4A28;
  font-size: 32rpx;
  text-align: center;
  background-repeat: no-repeat;
  width: 108px;
  margin: 12px;
}